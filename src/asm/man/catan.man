       #include &lt;stdio.h&gt;

       int
       main(int argc, char *argv[])
       {
	   double complex z, c, f;
	   double complex i = I;

	   if (argc != 3) {
	       fprintf(stderr, &quot;Usage: %s &lt;real&gt; &lt;imag&gt;
&quot;, argv[0]);
	       exit(EXIT_FAILURE);
	   }

	   z = atof(argv[1]) + atof(argv[2]) * I;

	   c = catan(z);
	   printf(&quot;catan() = %6.3f %6.3f*i
&quot;, creal(c), cimag(c));

	   f = (clog(1 + i * z) - clog(1 - i * z)) / (2 * i);
	   printf(&quot;formula = %6.3f %6.3f*i
&quot;, creal(f2), cimag(f2));

	   exit(EXIT_SUCCESS);
       }

